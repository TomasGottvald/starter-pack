    "graphql": "^15.5.1",
    "i18next": "^20.3.2",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-http-backend": "^1.2.6",
    "next": "11.0.0",
    "next-urql": "^3.1.0",
    "prettier": "^2.3.2",
    "react": "17.0.2",
    "react-docgen": "^5.4.0",
    "react-dom": "17.0.2",
    "react-i18next": "^11.11.0",
    "react-is": "^17.0.2",
    "react-styleguidist": "^11.1.7",
